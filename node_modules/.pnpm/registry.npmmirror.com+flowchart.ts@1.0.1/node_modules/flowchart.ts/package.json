{
  "name": "flowchart.ts",
  "version": "1.0.1",
  "author": "Mister-Hope",
  "description": "Flowchart support",
  "main": "./dist/flowchart.js",
  "module": "./dist/flowchart.mjs",
  "types": "./dist/flowchart.d.ts",
  "exports": {
    ".": {
      "types": "./dist/flowchart.d.ts",
      "import": "./dist/flowchart.mjs",
      "require": "./dist/flowchart.js",
      "default": "./dist/flowchart.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mister-Hope/flowchart.js.git"
  },
  "keywords": [
    "flowchart"
  ],
  "homepage": "https://github.com/Mister-Hope/flowchart",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/raphael": "^2.3.5",
    "raphael": "^2.3.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.5",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@types/node": "20.8.4",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "esbuild": "0.19.4",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "rollup": "4.0.2",
    "rollup-plugin-dts": "6.1.0",
    "rollup-plugin-esbuild": "6.1.0",
    "typescript": "5.2.2",
    "vite": "4.4.11"
  },
  "packageManager": "pnpm@8.9.0",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf dist/",
    "dev": "vite dev",
    "lint": "prettier --check --write . && eslint . --ext .ts --fix",
    "lint:check": "prettier --check . && eslint . --ext .ts",
    "prepublish": "pnpm clean && pnpm build"
  }
}