#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/uuuliu/Downloads/work/blog/node_modules/.pnpm/registry.npmmirror.com+marked@7.0.5/node_modules/marked/bin/node_modules:/Users/uuuliu/Downloads/work/blog/node_modules/.pnpm/registry.npmmirror.com+marked@7.0.5/node_modules/marked/node_modules:/Users/uuuliu/Downloads/work/blog/node_modules/.pnpm/registry.npmmirror.com+marked@7.0.5/node_modules:/Users/uuuliu/Downloads/work/blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/uuuliu/Downloads/work/blog/node_modules/.pnpm/registry.npmmirror.com+marked@7.0.5/node_modules/marked/bin/node_modules:/Users/uuuliu/Downloads/work/blog/node_modules/.pnpm/registry.npmmirror.com+marked@7.0.5/node_modules/marked/node_modules:/Users/uuuliu/Downloads/work/blog/node_modules/.pnpm/registry.npmmirror.com+marked@7.0.5/node_modules:/Users/uuuliu/Downloads/work/blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+marked@7.0.5/node_modules/marked/bin/marked.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+marked@7.0.5/node_modules/marked/bin/marked.js" "$@"
fi
